{
  "manifest_version": 2,
  "name": "Parsley",
  "version": "1.0.1",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "chrome_icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "*://www.google.com/reader/",
    "<all_urls>",
    "tabs",
    "storage"
  ]
}